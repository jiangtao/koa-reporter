{
  "name": "chef-template",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/halojs/chef-template"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halojs/chef-template/issues"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "node_modules/.bin/babel src -d dist",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava tests/index.js",
    "codecov": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "tag": "node .tag add $1",
    "tag:delete": "git tag -d $1 && git push origin --delete tag $1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "codecov": "^2.1.0",
    "koa": "^2.2.0",
    "koa-mount": "^3.0.0",
    "nyc": "^10.2.0",
    "request": "^2.81.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "ava": {
    "concurrency": 5,
    "babel": "inherit",
    "require": "babel-core/register"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "tests/*.js",
      "tests/**/*.js"
    ]
  }
}
